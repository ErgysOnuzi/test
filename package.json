{
  "name": "rest-express",
  "version": "1.0.1",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite --port 5000 --host 0.0.0.0",
    "build:client": "NODE_ENV=production vite build || (echo 'ERROR: Client build failed' && exit 1)",
    "build:server": "esbuild server/index.ts --bundle --platform=node --outfile=dist/server/index.cjs --format=cjs --target=node20 --external:sharp --external:better-sqlite3 --sourcemap --banner:js=\"try{require('node:buffer')}catch{};\" || (echo 'ERROR: Server build failed' && exit 1)",
    "build": "echo 'Building client...' && npm run build:client && echo 'Building server...' && npm run build:server && echo 'Build complete!'",
    "start": "NODE_ENV=production node dist/server/index.cjs",
    "preview": "vite preview --port 5000 --host 0.0.0.0",
    "postinstall": "echo 'Setup complete'",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push"
  },
